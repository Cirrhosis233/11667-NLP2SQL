{
    "model": "codellama/CodeLlama-7b-Instruct-hf",
    "dataset": "./datasets/sql-create-context-split",
    "prompt": "./prompts/prompt_v3_train.md",
    "output_dir": "./models/codellama-v4",
    "report_to": "wandb",
    "run_name": "codellama-v4",
    "eval_strategy": "steps",
    "eval_steps": 20,
    "logging_steps": 1,
    "max_seq_length": 4096,
    "bits": 4,
    "bnb_4bit_quant_type": "nf4",
    "r": 128,
    "lora_alpha": 128,
    "lora_dropout": 0.1,
    "target_modules": "all-linear",
    "bias": "none",
    "init_lora_weights": true,
    "task_type": "CAUSAL_LM",
    "num_train_epochs": 2,
    "save_strategy": "no",
    "bf16": true,
    "fp16": false,
    "gradient_checkpointing": true,
    "per_device_train_batch_size": 4,
    "per_device_eval_batch_size": 1,
    "lr_scheduler_type": "cosine",
    "learning_rate": 1e-4,
    "weight_decay": 0.001,
    "warmup_ratio": 0.03,
    "gradient_accumulation_steps": 4
}