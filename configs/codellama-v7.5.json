{
    "model": "codellama/CodeLlama-7b-Instruct-hf",
    "dataset": "./datasets/train_merge_150x5_750_diff_prompt",
    "prompt": "./prompts/prompt_v4_train.md",
    "output_dir": "./models/codellama-v7.5",
    "report_to": "wandb",
    "run_name": "codellama-v7.5",
    "eval_strategy": "no",
    "eval_steps": 10,
    "logging_steps": 1,
    "max_seq_length": 2048,
    "bits": 4,
    "bnb_4bit_quant_type": "nf4",
    "r": 64,
    "lora_alpha": 64,
    "lora_dropout": 0.1,
    "target_modules": [
        "q_proj",
        "v_proj"
    ],
    "bias": "none",
    "init_lora_weights": true,
    "task_type": "CAUSAL_LM",
    "num_train_epochs": 2,
    "save_strategy": "no",
    "bf16": true,
    "fp16": false,
    "gradient_checkpointing": true,
    "per_device_train_batch_size": 4,
    "per_device_eval_batch_size": 2,
    "lr_scheduler_type": "cosine",
    "learning_rate": 1e-4,
    "weight_decay": 0.001,
    "warmup_ratio": 0.03,
    "gradient_accumulation_steps": 4
}